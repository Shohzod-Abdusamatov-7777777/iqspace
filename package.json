{
	"name": "iqspace-platform",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"description": "IqSpace - Learning Management System built with Vue 3, TypeScript, Naive UI, and Tailwind CSS 4",
	"author": "IqSpace Team",
	"license": "MIT",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc && vite build",
		"build-only": "vite build",
		"preview": "vite preview",
		"json-server": "node db/server.cjs",
		"dev:all": "concurrently \"npm run dev\" \"npm run json-server\"",
		"type-check": "vue-tsc --noEmit"
	},
	"dependencies": {
		"@vueuse/core": "^13.4.0",
		"apexcharts": "^5.3.3",
		"axios": "^1.6.7",
		"json-server-auth": "^2.1.0",
		"lucide-vue-next": "^0.316.0",
		"naive-ui": "^2.38.1",
		"pinia": "^2.1.7",
		"vue": "^3.4.15",
		"vue-router": "^4.2.5",
		"vue3-apexcharts": "^1.8.0"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.0.0-alpha.4",
		"@types/node": "^20.11.5",
		"@vitejs/plugin-vue": "^5.0.3",
		"concurrently": "^8.2.2",
		"json-server": "^0.17.4",
		"tailwindcss": "^4.0.0-alpha.4",
		"typescript": "^5.3.3",
		"vite": "^5.0.12",
		"vue-tsc": "^1.8.27"
	},
	"keywords": [
		"vue3",
		"typescript",
		"naive-ui",
		"tailwindcss-4",
		"learning-management-system",
		"iqspace",
		"education",
		"pinia",
		"vue-router"
	]
}
